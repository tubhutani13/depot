<%= form.fields_for :address, address do |address_form| %>
  <div class="field">
    <%= address_form.label :state %>
    <%= address_form.text_field :state %>
  </div>

  <div class="field">
    <%= address_form.label :city %>
    <%= address_form.text_field :city %>
  </div>

  <div class="field">
    <%= address_form.label :country %>
    <%= address_form.text_field :country %>
  </div>

  <div class="field">
    <%= address_form.label :pincode %>
    <%= address_form.number_field :pincode %>
  </div>
<% end %>
